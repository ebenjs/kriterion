{
  "name": "@ebenjs/kriterion",
  "description": "Effortless validation library for Vue.js applications, ensuring accurate user data.",
  "license": "GNU General Public License v3.0",
  "keywords": [
    "Vue.js",
    "validation",
    "form validation",
    "data validation",
    "frontend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ebenjs/kriterion.git"
  },
  "author": "Your Name",
  "bugs": {
    "url": "https://github.com/ebenjs/kriterion/issues"
  },
  "version": "0.1.12",
  "type": "module",
  "files": [
    "dist"
  ],
  "private": false,
  "main": "./dist/kriterion.umd.js",
  "module": "./dist/kriterion.es.js",
  "exports": {
    ".": {
      "import": "./dist/kriterion.es.js",
      "require": "./dist/kriterion.umd.js"
    },
